
@foreach( var item in ViewBag.TopDirectories)
{
    @item.Name
    @foreach(var childDirectory in item.Children)
    {
        <div>
             <a asp-controller="Home" asp-action="Index" asp-route-id="@childDirectory.Id">@childDirectory.Name</a>
        </div>
    }
}
<form asp-action="Import" enctype="multipart/form-data">
    <div>
        @ViewBag.Error
    <br />
        Json File
        <input type ="file" name="jsonFile" />
    <br />
        <input type="submit" value="Import" />
    </div>
 </form>
